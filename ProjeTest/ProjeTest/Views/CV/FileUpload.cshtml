
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>FileUpload</title>
            <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">

</head>
<body>
      <nav class="navbar navbar-expand-lg navbar-light bg-light">
  <a class="navbar-brand" href="#">FRMRANDOM</a> 
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>

  <div class="collapse navbar-collapse" id="navbarSupportedContent">
    <ul class="navbar-nav mr-auto">
      <li class="nav-item ">
        <a class="nav-link" href="@Url.Action("AnaSayfa","KayitSistemi")">ANASAYFA</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="@Url.Action("Soru","Soru")">SORULAR</a>
      </li>
      <li class="nav-item">
        <a class="nav-link " href="/Profil/Profil/@ViewBag.Sessionn">PROFİL</a>
      </li>
       <li class="nav-item">
        <a class="nav-link " href="@Url.Action("FileUpload","CV")">Özgeçmiş Yükle</a>
      </li>
       <li class="nav-item">
        <a class="nav-link " href="@Url.Action("AdminLogin","Admin")">AdminPanel</a>
      </li>
    </ul>
    <ul class="navbar-nav mr-auto"style="float:right;">
            <li class="nav-item" >@ViewBag.Session</li>
    </ul>
  </div>
</nav>
<div class="mb-3">
     @using(Html.BeginForm("FileUpload","CV",FormMethod.Post,new { enctype = "multipart/form-data" })){
         @ViewBag.Result <br /> 
         @ViewBag.Bilgi<br />
    <input type="hidden" name="UyeID" value="@ViewBag.Sessionn"/>
  <label for="formFile" class="form-label">Özgeçmiş Dosyanızı Yükleyin</label>
  <input class="form-control" type="file" id="formFile" name="fromFile">
  <button type="submit" class="btn btn-primary btn-lg btn-block">Kaydet</button>
     }
</div>
                  <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
             <script src="https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
             <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
</body>
</html>
