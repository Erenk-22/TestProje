@model EntityLayer.Concrete.UyeSorülar
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Soru</title>
        <link rel="stylesheet" href="/css/Formm.css" >

</head>
<body>
 
    
  <div class="row">
    <div class="col-md-12">
          @using(Html.BeginForm("Soru","Soru",FormMethod.Post))
  {
      <input type="hidden" name="UyeID" value="@ViewBag.Sessionn"/>
      <input type="hidden" name="UyeMail" value="@ViewBag.UyeAd"/>
        <h1> Sorular </h1>
        
        <fieldset>
          
          <legend><span class="number">1</span> Üniversite Bilgisi</legend>
        
          <label for="name">Hangi Üniversiteden Mezunsunuz?:</label>
          <input type="text" id="name" name="SoruBir">
        
          <label for="email">Hangi Bölümden Mezunsunuz?:</label>
          <input type="text" id="mail" name="Soruİki">
       
          <label for="password">Gano Ortalamanız Kaçtı?:</label>
          <input type="text" id="password"name="SoruUc">
        
          <label>Seviye olarak kendinizi hangi seviyede görüyosunuz?:</label>
          <input type="radio" id="under_13" value="zayıf" name="SoruDort"><label for="under_13" class="light">0-2</label><br>
          <input type="radio" id="over_13" value="geliştirmeli" name="SoruDort"><label for="over_13" class="light">2-4</label><br>
          <input type="radio" id="over_14" value="iyi" name="SoruDort"><label for="over_14" class="light">4-6</label><br>
          <input type="radio" id="over_15" value="çokİyi" name="SoruDort"><label for="over_15" class="light">6-8</label><br>
          <input type="radio" id="over_16" value="Mükemmel" name="SoruDort"><label for="over_16" class="light">8-10</label>
        </fieldset>
        <fieldset>  
        
          <legend><span class="number">2</span>Program Bilgisi</legend>
          
         <label for="bio">Hangi Program Dillerini Biliyorsunuz?:</label>
          <textarea id="bio" name="SoruBes"></textarea>
        
         <label for="job">Aktif Olarak kaç Senedir Program yazıyorsunuz?:</label>
          <select id="job" name="SoruAlti">
              <option value="YeniProgramır">0-1 Yıl Arası</option>
              <option value="Geliştirilmeli">1-2 Yıl Arası</option>
              <option value="İyiSeviyede">2-4 Yıl Arası</option>
              <option value="Senior">5 ve üzeri Yıldır</option>
          </select>
        
         <label for="job">Kendinizi Hangi Sektörde Geliştirmek İstiyorsunuz?:</label>
          <select id="job" name="SoruYedi">
            <optgroup label="Web">
              <option value="frontend_developer">Front-End Developer</option>
              <option value="php_developer">PHP Developer</option>
              <option value="python_developer">Python Developer</option>
              <option value="rails_developer">Rails Developer</option>
              <option value="web_designer">Web Designer</option>
              <option value="wordpress_developer">Wordpress Developer</option>
            </optgroup>
            <optgroup label="Mobile">
              <option value="android_developer">Android Developer</option>
              <option value="ios_developer">IOS Developer</option>
              <option value="mobile_designer">Mobile Designer</option>
            </optgroup>
          </select>
           <label for="name">Bir Projeye Başlamadan önce ilk Olarak Ne yaparsın?:</label>
          <input type="text" id="name" name="SoruSekiz">
           <label for="name">Yaptığın Projelerden Biraz Bahsedermisin?:</label>
          <textarea id="bio" name="SoruDokuz"></textarea>
                  
          <label>Bu zamana kadar kaç kere takım çalışmasında bulundun?:</label>
          <input type="radio" id="under_17" value="Junior" name="SoruOn"><label for="under_17" class="light">Hiç bulunmadım</label><br>
          <input type="radio" id="over_18" value="Developer" name="SoruOn"><label for="over_18" class="light">0-5 proje arasında</label><br>
          <input type="radio" id="over_19" value="Senior" name="SoruOn"><label for="over_19" class="light">5 ve üste projede</label>
          
         </fieldset>
       
        <button type="submit">Formu Gönder</button>
        
         }
        </div>
      </div>
      

             <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
             <script src="https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
             <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
</body>
</html>
